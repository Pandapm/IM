    "webpack-dev-server": "^2.9.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage",
    "dev": "cross-env NODE_ENV=development nodemon --watch out --exec  './node_modules/.bin/electron' ./src/main.js",
    "commit":"git-cz"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "@types/react-router": "^4.0.20",
    "@types/react-router-dom": "^4.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "commitizen": "^2.9.6",
    "cross-env": "^5.1.3",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "less": "^2.3.1",
    "less-loader": "^4.0.5",
    "nodemon": "^1.14.11",
    "react-loadable": "^5.3.1",
    "react-test-renderer": "^16.2.0",
    "ts-node": "^5.0.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "config": {
    "commitizen": {
        "path": "node_modules/cz-conventional-changelog"
      }
    },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/__tests__/**/*.ts?(x)",
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "mjs",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "/Users/admin/hello/tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
