    "scripts": {
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js --env=jsdom",
        "test:coverage": "node scripts/test.js --env=jsdom --coverage",
        "dev": "cross-env NODE_ENV=development nodemon --watch out --exec  './node_modules/.bin/electron' ./src/main.js",
        "commit": "git-cz",
        "tools": "electron ./node_modules/electron-toolkit"
    },
    "devDependencies": {
        "@types/enzyme": "^3.1.13",
        "@types/jest": "^22.2.0",
        "@types/node": "^9.4.7",
        "@types/react": "^16.4.8",
        "@types/react-dom": "^16.0.7",
        "@types/react-router": "^4.0.30",
        "@types/react-router-dom": "^4.3.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "commitizen": "^2.10.1",
        "cross-env": "^5.2.0",
        "cz-conventional-changelog": "^2.1.0",
        "electron-builder": "^20.27.1",
        "electron-toolkit": "^1.0.24",
        "enzyme": "^3.4.1",
        "enzyme-adapter-react-16": "^1.2.0",
        "extract-text-webpack-plugin": "^3.0.2",
        "less": "^2.3.1",
        "less-loader": "^4.1.0",
        "nodemon": "^1.18.3",
        "react-loadable": "^5.4.0",
        "react-test-renderer": "^16.4.2",
        "ts-node": "^5.0.1",
        "webpack-manifest-plugin": "^1.3.2"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        }
    },
    "engines": {
        "node": ">=7.6.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "<rootDir>/config/setupTests.js"
        ],
        "testMatch": [
            "<rootDir>/test/**/__tests__/**/*.ts?(x)",
            "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web"
        },
        "moduleFileExtensions": [
            "mjs",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "web.js",
            "js",
            "web.jsx",
            "jsx",
            "json",
            "node"
        ],
        "globals": {
            "ts-jest": {
                "tsConfigFile": "/Users/admin/hello/tsconfig.test.json"
            }
        }
    },
    "babel": {
        "presets": [
            "react-app"
        ],
        "plugins": [
            "syntax-dynamic-import"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "description": "",
    "build": {
        "appId": "com.electron.hello",
        "copyright": "Copyright Â© 2018 guiqing",
        "productName": "hello",
        "electronVersion": "1.8.3",
        "asar": true,
        "directories": {
            "app": "/Users/admin/hello",
            "output": "dist"
        },
        "mac": {
            "category": "public.app-category.business",
            "target": [
                {
                    "target": "dmg",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        }
    }
}
