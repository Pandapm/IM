  "private": true,
  "main": "main.ts",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "axios": "^0.18.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "electron": "^2.0.9",
    "file-loader": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.4.9",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "mobx": "^5.1.1",
    "mobx-react": "^5.2.8",
    "mobx-react-router": "^4.0.5",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.5.0",
    "react-dev-utils": "^5.0.2",
    "react-dom": "^16.5.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-transition-group": "^2.4.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^23.1.4",
    "ts-loader": "^3.5.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3",
    "url-loader": "^1.1.1",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage",
    "dev": "cross-env NODE_ENV=development nodemon --watch out --exec  './node_modules/.bin/electron' ./src/main.js",
    "commit": "git-cz",
    "tools": "electron ./node_modules/electron-toolkit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.10",
    "@storybook/addon-links": "^3.4.10",
    "@storybook/addons": "^3.4.10",
    "@storybook/react": "^3.4.10",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "babel-core": "^6.26.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-runtime": "^6.26.0",
    "commitizen": "^2.10.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "electron-builder": "^20.28.4",
    "electron-toolkit": "^1.0.24",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint-config-react-app": "^2.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.4",
    "react-loadable": "^5.5.0",
    "react-test-renderer": "^16.5.0",
    "ts-node": "^7.0.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=7.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "D:\\Code\\my-app\\tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "description": "",
  "build": {
    "appId": "com.electron.hello",
    "copyright": "Copyright Â© 2018 guiqing",
    "productName": "hello",
    "electronVersion": "2.0.9",
    "asar": true,
    "directories": {
      "app": "/Users/admin/hello",
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.business",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
